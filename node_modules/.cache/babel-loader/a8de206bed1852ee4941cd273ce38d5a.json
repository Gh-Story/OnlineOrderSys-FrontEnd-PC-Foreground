Content":["<template>\r\n\t<div class='taskManage'>\r\n\t  <el-row>\r\n\t\t  <img src=\"../../../../static/任务迎取红包.jpg\" style=\"width: 100%;height: 200px;\">\r\n\t  </el-row>\r\n\t  <h3 style=\"padding-bottom: 10px;\">任务中心:</h3>\r\n\t  <div v-for=\"item in tasks\">\r\n\t\t<el-card body-style=\"padding:0px\">\r\n\t\t\t<el-col :span=\"6\">\r\n\t\t\t\t<h2 style=\"padding-top: 5%;\">{{item.name}}</h2>\r\n\t\t\t</el-col>\r\n\t\t\t<el-col :span=\"6\">\r\n\t\t\t\t<h3 style=\"padding-top: 8%;\">今日下{{item.need}}单</h3>\r\n\t\t\t</el-col>\n\t\t\t<el-col :span=\"10\">\r\n\t\t\t\t<h3 style=\"padding-top: 5%;color: red;\">奖励：{{item.redPacket}}</h3>\r\n\t\t\t</el-col>\r\n\t\t\t<el-col :span=\"2\">\r\n\t\t\t\t<div style=\"position: relative;\" @click=\"getTask\"> \r\n\t\t\t\t\t<img\r\n\t\t\t\t\tsrc=\"../../../../static/红包包.jpg\" \r\n\t\t\t\t\tclass=\"image\"\r\n\t\t\t\t\theight=\"60px\">\r\n\t\t\t\t\t<span style=\"cursor: default; user-select:none; position: absolute; bottom: 24px; left: 3px;\r\n\t\t\t\t\tfont-size: 8px;font-family:'微软雅黑';color: white;\">领取任务</span> \r\n\t\t\t\t</div>\r\n\t\t\t</el-col>\r\n\t\t</el-card><br />\r\n\t  </div>\r\n\t  <h3 style=\"padding-bottom: 10px;\">我的任务:</h3>\r\n\t  <div v-for=\"item in myTasks\">\r\n\t  \t\t<el-card body-style=\"padding:0px\">\r\n\t  \t\t\t<el-col :span=\"6\">\r\n\t  \t\t\t\t<h2 style=\"padding-top: 5%;\">{{item.name}}</h2>\r\n\t  \t\t\t</el-col>\r\n\t  \t\t\t<el-col :span=\"6\">\r\n\t  \t\t\t\t<h3 style=\"padding-top: 5%;\">今日下{{item.need}}单</h3>\r\n\t\t\t\t\t<h4 style=\"color: grey;\" >已完成{{item.finished}}单</h4>\r\n\t  \t\t\t</el-col>\r\n\t  \t\t\t<el-col :span=\"10\">\r\n\t  \t\t\t\t<h3 style=\"padding-top: 5%;color: red;\">奖励：{{item.redPacket}}</h3>\r\n\t  \t\t\t</el-col>\r\n\t  \t\t\t<el-col :span=\"2\">\r\n\t  \t\t\t\t<div style=\"position: relative;\" @click=\"getRedpack\">\r\n\t  \t\t\t\t\t<img\r\n\t  \t\t\t\t\tsrc=\"../../../../static/红包包.jpg\" \r\n\t  \t\t\t\t\tclass=\"image\"\r\n\t  \t\t\t\t\theight=\"60px\">\r\n\t  \t