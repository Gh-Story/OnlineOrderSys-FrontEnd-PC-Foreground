t\tthis.seasonFood='';\r\n\t\t\t\tthis.seasonFood+= r.data.msg.food1.foodName;\r\n\t\t\t\tthis.seasonFood+='     ';\r\n\t\t\t\tthis.seasonFood+=r.data.msg.food2.foodName;\r\n\t\t\t\t//this.notice=r.data.msg.NewNotice.noticeContent;\r\n\t\t\t});\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}